#include <iostream>
using namespace std;

int main()
{
	int n, a, past_a, i, t, arr[1000];
	t = 0;
	i = 0;
	cin >> n;
	for (int k = 0; k < n; k++) {
		if (k != 0) {
			past_a = a;
		}
		cin >> a;
		if (a == 1) {
			t++;
			if (k != 0) {
				arr[i] = past_a;
				i++;
			}
		}
		if (k == n - 1) {
			arr[i] = a;
			i++;
		}
	}
	cout << t << endl;
	for (int j = 0; j < i; j++) {
		cout << arr[j] << " ";
	}
    return 0;
}
