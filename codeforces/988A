#include <iostream>
#include <set>
using namespace std;

int main() {
    int n, k, a, new, old;
    set <int> s;
    set <int> result;
    cin >> n;
    cin >> k;
    for (int i = 0; i < n; i++) {
        cin >> a;
        old = s.size();
        s.insert(a);
            new = s.size();
            if (old < new && new <= k) {
                result.insert(i + 1);
            }
        }
        if (s.size() >= k) {
            cout << "Yes" << endl;
            for (set<int>::iterator i = result.begin(); i != result.end(); ++i) {
                cout << *i << ", ";
            }
        } else {
            cout << "No";
       }
    return 0;
}
